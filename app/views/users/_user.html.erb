<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email_address %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= user.phone %>
  </p>

  <p>
    <strong>Entry date:</strong>
    <%= user.entry_date %>
  </p>

  <p>
    <strong>Active:</strong>
    <%= user.active %>
  </p>

</div>
